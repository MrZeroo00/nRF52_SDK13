pipeline {
  agent any
  stages {
    stage('Make') {
      steps {
          sh 'make -C examples/ble_peripheral/service_handson/pca10040/s132/armgcc/'
      }
    }
    stage('Archiving') {
      steps {
        archiveArtifacts artifacts: 'xamples/ble_peripheral/service_handson/pca10040/s132/armgcc/_build/*.hex', onlyIfSuccessful: true
	archiveArtifacts artifacts: 'xamples/ble_peripheral/service_handson/pca10040/s132/armgcc/_build/*.out', onlyIfSuccessful: true
      }
    }
  }
}
